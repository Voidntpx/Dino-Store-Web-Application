	<div class="cart-wrap">
		<div class="container">
	        <div class="row">
			    <div class="col-md-12">
			        <div class="main-heading mb-10"><h1>My wishlist</h1></div>
			        <div class="table-wishlist">
				        <table cellpadding="0" cellspacing="0" border="0" width="100%">
				        	<thead>
					        	<tr>
					        		<th width="45%"><h3>Product Name</h3></th>
					        		<th width="15%"><h3>Unit Price</h3></th>
					        		<th width="15%"><h3>Stock Status</h3></th>
					        		<th width="15%"></th>
					        		<th width="10%"></th>
					        	</tr>
					        </thead>
					        <tbody>
                                {{# each wishitems}}
                                 {{# each this}}
					        	<tr>
					        		<td width="45%">
					        			<div class="display-flex align-center">
		                                    <div class="img-product">
		                                       <a href="/productdetail/{{this.productid}}"><img src="https://icon-library.com/images/user-manual-icon/user-manual-icon-26.jpg" alt="" class="mCS_img_loaded"></a>
		                                    </div>
		                                    <div class="name-product">
		                                        <h3>{{this.title}}</h3>
		                                    </div>
	                                    </div>
	                                </td>
					        		<td width="15%" class="price"><h3>${{this.price}}</h3></td>
					        		<td width="15%"><span class="in-stock-box">In Stock</span></td>
					        		<td width="15%"> <div><a href="/additemtocart3/{{this.productid}}" class="btn btn-success">Add to cart</a></div></td>
					        		<td width="10%" class="text-center"><a href="/deletewish/{{this._id}}" class="trash-icon" style="color: red;"><i class="far fa-trash-alt"></i></a></td>
					        	
                                </tr>   	
                                {{/each}}
                                {{/each}}
				        	</tbody>
				        </table>
				    </div>
			    </div>
			</div>
		</div>
	</div>
    <br><br><br><br><br><br><br>
	
